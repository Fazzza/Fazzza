  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(title: Text("Karakee Cafe")),
      body: ListView(
        children: [
          Text("Welcome to Karakee Cafe"),
          // Add more content here
        ],
      ),
    );
  }
}
import React from 'react';
